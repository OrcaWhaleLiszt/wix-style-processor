{
  "private": false,
  "name": "wix-style-processor",
  "description": "An alternative Wix Styles TPA processor",
  "version": "3.0.0",
  "author": {
    "name": "Eran Shabi",
    "email": "erans@wix.com",
    "url": ""
  },
  "main": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "yoshi start --entry-point=./test/mock/start-fake-server.js",
    "build": ":",
    "pretest": "yoshi lint && yoshi build",
    "test:unit": "yoshi test --mocha",
    "test:e2e": "yoshi test --protractor",
    "test": "npm run test:unit && npm run test:e2e",
    "release": "yoshi release"
  },
  "dependencies": {
    "@types/lodash": "^4.14.64",
    "babel-runtime": "^6.22.0",
    "color": "^1.0.3",
    "lodash": "^4.17.4",
    "parse-css-font": "^2.0.2",
    "stylis": "^3.2.4"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/chai-as-promised": "^0.0.29",
    "@types/mocha": "^2.2.39",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.3.0",
    "sinon": "^2.2.0",
    "ts-node": "^3.2.0",
    "velocity": "^0.7.2",
    "yoshi": "latest"
  },
  "yoshi": {
    "entry": {
      "app": "../test/test-setup/app.ts",
      "style-processor": "./index.ts"
    },
    "separateCss": false
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "wix/esnext"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
