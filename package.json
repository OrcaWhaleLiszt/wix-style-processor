{
  "private": false,
  "name": "wix-style-processor",
  "description": "An alternative Wix Styles TPA processor",
  "version": "3.0.0",
  "author": {
    "name": "Eran Shabi",
    "email": "erans@wix.com",
    "url": ""
  },
  "main": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "haste start --entry-point=./test/mock/start-fake-server.js",
    "build": ":",
    "pretest": "haste lint && haste build",
    "test:unit": "haste test --mocha",
    "test:e2e": "haste test --protractor",
    "test": "npm run test:unit && npm run test:e2e",
    "release": "haste release"
  },
  "dependencies": {
    "color": "^1.0.3",
    "parse-css-font": "^2.0.2",
    "stylis": "^3.2.4"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/chai-as-promised": "^0.0.29",
    "@types/mocha": "^2.2.39",
    "chai": "^3.5.0",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.3.0",
    "sinon": "^2.2.0",
    "ts-node": "^3.2.0",
    "typescript": "^2.4.2",
    "velocity": "^0.7.2",
    "haste-preset-yoshi": "latest"
  },
  "yoshi": {
    "entry": {
      "app": "../test/test-setup/app.ts",
      "style-processor": "./index.ts"
    },
    "separateCss": false
  },
  "eslintConfig": {
    "extends": "wix/esnext"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "haste": {
    "preset": "yoshi"
  }
}
