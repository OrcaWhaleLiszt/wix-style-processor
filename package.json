{
  "name": "wix-style-processor",
  "version": "1.0.0",
  "description": "An alternative Wix Styles TPA processor",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix/wix-style-processor"
  },
  "scripts": {
    "test": "mocha --require babel-register \"test/**/*.test.js\"",
    "test:watch": "mocha --watch --require babel-register \"test/**/*.test.js\"",
    "compile": "rm -rf dist/ && babel -d dist/ src/",
    "build": "npm run compile",
    "bundle": "webpack",
    "release": "wnpm-release"
  },
  "private": false,
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "color": "^0.11.3",
    "lodash": "^4.15.0",
    "mocha": "^3.0.2",
    "parse-css-font": "^2.0.2",
    "q": "^1.4.1"
  },
  "devDependencies": {
    "@wix/wnpm-ci": "*",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-rewire": "^1.0.0-rc-5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "sinon": "^1.17.5",
    "webpack": "^1.13.1"
  }
}
